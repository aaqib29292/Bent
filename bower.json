{
  "name": "bent",
  "description": "A single page responsive website built using bootstrap framework, bower package manager and grunt task automator",
  "main": "index.html",
  "authors": [
    "Aaqib Parvez"
  ],
  "license": "MIT",
  "keywords": [
    "html",
    "css",
    "bootstrap",
    "jquery",
    "bower",
    "grunt"
  ],
  "homepage": "",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "bootstrap": "^3.3.7",
    "font-awesome": "fontawesome#^4.7.0",
    "owlcarousel": "^1.3.2",
    "wow": "wowjs#^1.1.2",
    "animate.css": "^3.5.2",
    "normalize-css": "^5.0.0",
    "fitvids": "^1.2.0"
  },
  "overrides": {
    "bootstrap": {
      "main": [
        "dist/js/bootstrap.js",
        "dist/css/bootstrap.css",
        "less/bootstrap.less"
      ]
    },
    "font-awesome": {
      "main": [
        "css/font-awesome.css"
      ]
    },
    "owlcarousel": {
      "main": [
        "owl-carousel/owl.carousel.js",
        "owl-carousel/owl.theme.css",
        "owl-carousel/owl.carousel.css"
      ]
    }
  }
}
